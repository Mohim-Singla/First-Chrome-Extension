{
    "manifest_version": 2,
    "name": "Motivatanum",
    "version": "1.0",
    "web_accessible_resources": [
        "Images/*.jpg"
    ],
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "index.png"
    }
}